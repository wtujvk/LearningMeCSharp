
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@(ViewData["Title"]?? "wtujvk.LearningMeCSharp.MvcDemo")</title>
    <link href="~/www/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    @RenderSection("Styles", required: false)
</head>
<body>
    <div class="container">
        <div class="page-header">
            <a class="btn btn-default btn-link btn-primary" href="/Home/Index">首页,判断session</a>
            <a class="btn btn-default btn-link btn-primary" href="/Home/LoginIn">登入,设置session</a>
            <a class="btn btn-default btn-link btn-primary" href="/Home/LogOut">登出，清除session</a>
            <a class="btn btn-default btn-link btn-primary" href="/Home/ClearAll">清除当前全部session</a>
        </div>
        <div>  @RenderBody()</div>
      
    </div>
    @RenderSection("Scripts", required: false)
</body>
</html>
